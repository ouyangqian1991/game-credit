<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog 
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

<property name="autoIncrement" value="true" dbms="mysql"/>

<changeSet id="2018-08-09-init-test" author="ouyangqian">
	
	<comment>liqui的测试---新建一张表</comment>
	<createTable tableName="liqui_user">
		<column name="id" type="bigint" autoIncrement="${autoIncrement}" remarks="用户id,主键自增长">
			<constraints primaryKey="true" nullable="false"/>
		</column>	
		<column name="u_name" type="varchar(25)" remarks="用户的姓名">
			<constraints nullable="false"/>
		</column>
		<column name="u_pwd" type="varchar(50)" remarks="用户的密码">
			<constraints nullable="false"/>
		</column>
	</createTable>
	
</changeSet>
    
</databaseChangeLog>